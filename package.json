{
  "private": true,
  "productName": "Simple Fable App",
  "version": "1.0.0",
  "main": "app/main.js",
  "scripts": {
    "start": "cd src/Main && dotnet fable webpack --port free -- -w --config ../../webpack.config.js",
    "build": "cd src/Main && dotnet fable webpack --port free -- -p --config ../../webpack.config.js",
    "launch": "electron .",
    "release": "electron-packager . --out release --overwrite"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "file-size": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "electron": "^1.7.5",
    "electron-packager": "^12.1.0",
    "fable-loader": "^1.0.7",
    "fable-utils": "^1.0.2",
    "loglevel": "^1.4.1",
    "webpack": "^3.5.5"
  }
}
